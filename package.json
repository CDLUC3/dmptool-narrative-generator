{
  "name": "dmptool-narrative-generator",
  "description": "Service that accepts a DMP as JSON and renders a narrative document",
  "author": "University of California Curation Center (UC3)<dmptool@ucop.edu>",
  "license": "MIT",
  "version": "1.0.0",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/CDLUC3/dmptool-narrative-generator.git"
  },
  "scripts": {
    "aws_start": "node ./dist/server.js",
    "build": "rm -rf ./dist && npm run compile",
    "compile": "tsc",
    "dev": "ts-node-dev --respawn ./src/server.ts",
    "lint": "npx eslint . --ignore-pattern dist/",
    "test": "jest --coverage",
    "start": "npm run build && nodemon ./dist/server.js",
    "prepare": "is-ci || npx husky install",
    "trivy-high": "./src/scripts/trivy-high.sh",
    "trivy-med": "./src/scripts/trivy-med.sh"
  },
  "overrides": {
    "tar-fs": "3.1.1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.873.0",
    "@aws-sdk/util-dynamodb": "3.873.0",
    "@dmptool/types": "1.2.3",
    "@elastic/ecs-pino-format": "1.5.0",
    "@turbodocx/html-to-docx": "1.13.5",
    "cookie-parser": "1.4.7",
    "csv-stringify": "6.6.0",
    "domelementtype": "2.3.0",
    "dotenv": "17.2.1",
    "express": "5.1.0",
    "express-jwt": "8.5.1",
    "handlebars": "4.7.8",
    "htmlparser2": "10.0.0",
    "jsonwebtoken": "9.0.2",
    "mysql2": "3.14.4",
    "pino": "9.9.0",
    "pluralize": "8.0.0",
    "puppeteer": "24.17.0"
  },
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@types/eslint__js": "8.42.3",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.3.0",
    "@types/pino": "7.0.4",
    "eslint": "9.34.0",
    "fast-check": "4.2.0",
    "husky": "9.1.7",
    "jest": "30.0.5",
    "jest-expect-message": "1.1.3",
    "supertest": "7.1.4",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.42.0"
  }
}
